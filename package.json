{
    "dependencies": {
        "cookie-parser": "^1.4.6",
        "cors": "^2.8.5",
        "eslint-plugin-prettier": "^4.2.1",
        "ethers": "^6.3.0",
        "express": "^4.18.2",
        "express-session": "^1.17.3"
    },
    "name": "StorySwap_Backend",
    "version": "1.0.0",
    "main": "index.js",
    "scripts": {
        "test": "jest",
        "start": "ts-node --require tsconfig-paths/register ./src/index.ts",
        "start:prd": "node build/src/index.js",
        "build": "babel src --extensions \".js,.ts\" --out-dir build --copy-files",
        "start:dev": "yarn ts-node-dev -r dotenv/config -r tsconfig-paths/register --inspect --transpile-only ./src/index.ts",
        "lint": "eslint \"src/**/*.{js,ts,json, yaml}\" ",
        "lint:fix": "eslint --fix \"src/**/*.{js,ts,json}\" ",
        "format": "prettier --write \"src/**/*.{js,ts,css,md,json, yaml}\" --config ./.prettierrc"
    },
    "repository": "https://github.com/MauroSilva17/StorySwap_Backend.git",
    "author": "Mauro <m.henriquesilva18@gmail.com>",
    "license": "MIT",
    "devDependencies": {
        "@types/cookie-parser": "^1.4.3",
        "@types/cors": "^2.8.13",
        "@types/express": "^4.17.17",
        "@types/express-session": "^1.17.7",
        "@types/node": "^18.16.3",
        "@typescript-eslint/eslint-plugin": "^5.59.2",
        "@typescript-eslint/parser": "^5.59.2",
        "babel": "^6.23.0",
        "babel-cli": "^6.26.0",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-decorators": "^7.21.0",
        "@babel/preset-env": "^7.20.2",
        "@babel/preset-typescript": "^7.21.0",
        "crypto": "^1.0.1",
        "dotenv": "^16.0.3",
        "eslint": "^8.39.0",
        "eslint-config-prettier": "^8.8.0",
        "ts-node-dev": "^2.0.0",
        "prettier": "^2.8.8",
        "rimraf": "^5.0.0",
        "ts-node": "^10.9.1",
        "tsconfig-paths": "^4.2.0",
        "typescript": "^5.0.4"
    }
}
